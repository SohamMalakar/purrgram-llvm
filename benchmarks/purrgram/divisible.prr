def count_divisible() -> int:
    var count: int = 0
    var i: int = 1
    while i < 1000000001:
        if i % 3 == 0:
            count += 1
        elif i % 5 == 0:
            count += 1
        end
        i += 1
    end
    print("Count of numbers divisible by 3 or 5: %d", count)
    return 0
end

count_divisible()